<div class="listnewPage">
  <div class="container">
    <%= form_with model: @room, url: { action: :update }, class: "new_list", local: true do |f| %>
      <% if @room.errors.any? %>
        <p class="text-danger">タイトルは1～255文字以内で入力してください</p>
      <% end %>
      <%= f.label :@theme.id %>
      <%= f.hidden_field :User01Role, value: "oo" %>
      <div class="text-center"><%= f.submit "更新", class: "submitBtn" %></div>
    <% end %>
  </div>
</div>